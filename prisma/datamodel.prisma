type User {
  id: ID! @unique
  email: String! @unique
  password: String!
  googleFitAccount: GoogleFitAccount
  createdAt: DateTime!
  updatedAt: DateTime!
}

type GoogleFitAccount {
  id: ID! @unique
  refreshToken: String!
  accessToken: GoogleFitAccessToken!
}

type GoogleFitAccessToken {
  id: ID! @unique
  account_id: ID!
  value: String!
  expiration: DateTime!
}