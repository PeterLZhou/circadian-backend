{
  "name": "circadian-backend",
  "scripts": {
    "start": "npx dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "prod": "ts-node src/index.ts",
    "dev": "npm-run-all --parallel gen start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "gen": "graphql prepare && graphql codegen",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "updatedatabase": "prisma deploy"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^6.0.0",
    "graphql-yoga": "1.16.1",
    "jsonwebtoken": "8.3.0",
    "prisma-binding": "2.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.16.5",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3",
    "prisma": "1.14.2",
    "rimraf": "2.6.2",
    "ts-node": "6.2.0",
    "typescript": "2.9.2"
  }
}
